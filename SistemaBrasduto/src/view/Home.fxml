<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="1280.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.HomeController">
    <stylesheets>
        <URL value="@/css/home.css" />
    </stylesheets>
    <children>
        <BorderPane prefHeight="720.0" prefWidth="1280.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <top>
                <HBox alignment="CENTER_LEFT" prefHeight="71.0" prefWidth="1280.0" BorderPane.alignment="CENTER">
                    <children>
                        <Button focusTraversable="false" mnemonicParsing="false" prefHeight="79.0" prefWidth="246.0" />
                        <Button fx:id="btnHome" mnemonicParsing="false" onAction="#btnHomeOnClicked" prefHeight="79.0" prefWidth="119.0" text="HOME" />
                        <Button fx:id="btnEmployee" mnemonicParsing="false" onAction="#goEmployee" prefHeight="79.0" prefWidth="119.0" text="Funcionarios" />
                    </children>
                </HBox>
            </top>
            <bottom>
                <AnchorPane prefHeight="33.0" prefWidth="1280.0" BorderPane.alignment="CENTER" />
            </bottom>
         <center>
            <AnchorPane fx:id="boxContent" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </center>
        </BorderPane>
    </children>
</AnchorPane>
